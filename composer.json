{
    "name": "ryssbowh/craft-restrict-deletions",
    "description": "Restrict deletion of elements when they are referenced in other elements",
    "type": "craft-plugin",
    "keywords": [
        "cms",
        "craftcms",
        "restrict",
        "delete",
        "deletion"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "The Web Puzzlers",
            "homepage": "https://github.com/ryssbowh"
        }
    ],
    "require": {
        "craftcms/cms": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Ryssbowh\\RestrictDeletion\\": "src/"
        }
    },
    "extra": {
        "name": "Restrict related deletions",
        "handle": "restrict-deletion",
        "developer": "The Web Puzzlers",
        "developerUrl": "https://puzzlers.run",
        "documentationUrl": "https://puzzlers.run/plugins/restrict-element-deletions/1.x",
        "class": "Ryssbowh\\RestrictDeletion\\RestrictDeletion"
    }
}
