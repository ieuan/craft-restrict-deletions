{
    "name": "ryssbowh/craft-restrict-deletions",
    "description": "View elements relations and restrict deletion of elements when they are referenced in other elements",
    "type": "craft-plugin",
    "keywords": [
        "cms",
        "craftcms",
        "restrict",
        "delete",
        "deletion",
        "related",
        "relation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "The Web Puzzlers",
            "homepage": "https://github.com/ryssbowh"
        }
    ],
    "require": {
        "craftcms/cms": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Ryssbowh\\RestrictDeletion\\": "src/"
        }
    },
    "extra": {
        "name": "Restrict element deletions",
        "handle": "restrict-deletion",
        "developer": "The Web Puzzlers",
        "developerUrl": "https://www.webpuzzlers.co.uk",
        "documentationUrl": "https://www.webpuzzlers.co.uk/plugins/restrict-element-deletions/2.x",
        "class": "Ryssbowh\\RestrictDeletion\\RestrictDeletion"
    }
}
